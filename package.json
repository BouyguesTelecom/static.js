{
  "name": "@bouygues-telecom/staticjs",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "config",
    "helpers",
    "scripts",
    "package.json",
    "README.md"
  ],
  "bin": {
    "create-staticjs-app": "./scripts/create-static-app.js",
    "bt-staticjs": "./scripts/cli.js"
  },
  "scripts": {
