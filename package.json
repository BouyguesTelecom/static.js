    "cli-spinner": "^0.2.10",
    "commander": "^14.0.0",
    "giget": "^2.0.0",
    "nodemon": "^3.1.10",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5"
  },
  "volta": {
    "node": "24.1.0"
  },
  "workspaces": [
    "templates/react"
  ],
  "devDependencies": {
    "@types/express": "^5.0.3"
  }
