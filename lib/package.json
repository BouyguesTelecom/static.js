{
  "name": "@bouygues-telecom/staticjs",
  "version": "1.0.17",
  "main": "./_build/server/index.mjs",
  "exports": {
    ".": {
      "types": "./_build/server/index.d.ts",
      "default": "./_build/server/index.mjs"
    },
    "./server": {
      "types": "./_build/server/index.d.ts",
      "default": "./_build/server/index.mjs"
    },
    "./config": {
      "types": "./_build/server/config/index.d.ts",
      "default": "./_build/server/config/index.mjs"
    },
    "./helpers": {
      "types": "./_build/helpers/cachePages.d.ts",
      "default": "./_build/helpers/cachePages.mjs"
    },
    "./head": {
      "types": "./_build/components/Head.d.ts",
      "default": "./_build/components/Head.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "server": [
        "./_build/server/index.d.ts"
      ],
      "config": [
        "./_build/server/config/index.d.ts"
      ],
      "helpers": [
        "./_build/helpers/cachePages.d.ts"
      ],
      "head": [
        "./_build/components/Head.d.ts"
      ]
    }
  },
  "files": [
    "_build",
    "package.json",
    "README.md"
  ],
  "bin": {
    "create-staticjs-app": "./_build/scripts/create-static-app.mjs",
    "bt-staticjs": "./_build/scripts/cli.mjs",
    "static": "./_build/scripts/cli.mjs",
    "generate-test-multiapps": "./_build/scripts/generate-test-multiapps.mjs"
  },
  "scripts": {
    "build": "rm -rf _build && tsc && node scripts/post-build.mjs && chmod +x _build/scripts/*.mjs && cp -r server/static _build/server/",
    "dev": "npm run build && cd ../templates/react && npm run dev"
  },
  "dependencies": {
    "chokidar": "^4.0.1",
    "cli-spinner": "^0.2.10",
    "commander": "^14.0.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "express-rate-limit": "^8.0.1",
    "giget": "^2.0.0",
    "helmet": "^8.0.0",
    "nodemon": "^3.1.10",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.0.0",
    "ws": "^8.18.0"
  },
  "peerDependencies": {
    "vite": "^6.0.0 || ^7.0.0",
    "sass": "^1.77.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "24.1.0"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.7",
    "@types/cli-spinner": "^0.2.3",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/ws": "^8.18.1"
  }
}
